# Managed by NetApp E-Series Ansible

# Load IPoIB
IPOIB_LOAD={{ "yes" if eseries_ib_base_ipoib_enabled == True or eseries_ib_base_iser_enabled == True else "no" }}
# Load SRP (SCSI Remote Protocol initiator support) module
SRP_LOAD={{ "yes" if eseries_ib_base_srp_enabled == True else "no" }}
# Load SRPT (SCSI Remote Protocol target support) module
SRPT_LOAD={{ "yes" if eseries_ib_base_srp_enabled == True else "no" }}
# Load iSER (iSCSI over RDMA initiator support) module
ISER_LOAD={{ "yes" if eseries_ib_base_iser_enabled == True else "no" }}
# Load iSERT (iSCSI over RDMA target support) module
ISERT_LOAD={{ "yes" if eseries_ib_base_iser_enabled == True else "no" }}
# Load RDS (Reliable Datagram Service) network protocol
RDS_LOAD=no
# Load NFSoRDMA client transport module
XPRTRDMA_LOAD=no
# Load NFSoRDMA server transport module
SVCRDMA_LOAD=no
# Load Tech Preview device driver modules
TECH_PREVIEW_LOAD=no
